			<class name="com.test.espanol.desktop.RAQSearchInventoryEsp" />
			<class name="com.test.espanol.desktop.RAQStandaloneEsp" />

			<class name="com.test.espanol.mobile.RAQBuildAndPriceEspMobile" />
			<class name="com.test.espanol.mobile.RAQLocalSpecialEspMobile" />
			<class name="com.test.espanol.mobile.RAQSearchInventoryEspMobile" />
